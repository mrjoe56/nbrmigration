<?xml version="1.0"?>
<extension key="nbrmigration" type="module">
  <file>nbrmigration</file>
  <name>NIHR BioResource Migration</name>
  <description>CiviCRM specific extension fior NIHR BioResource migration (to be uninstalled after migration)</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Erik Hommel (CiviCooP)</author>
    <email>erik.hommel@civicoop.org</email>
  </maintainer>
  <urls>
